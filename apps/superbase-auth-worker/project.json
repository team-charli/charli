{
  "name": "superbase-auth-worker",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/superbase-auth-worker/src",
  "projectType": "application",
  "targets": {
    "serve": {
      "executor": "@naxodev/nx-cloudflare:serve",
      "options": {
        "port": 8787
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/superbase-auth-worker/**/*.ts"
        ]
      }
    },
    "deploy": {
      "executor": "@naxodev/nx-cloudflare:deploy"
    }
  },
  "tags": []
}
